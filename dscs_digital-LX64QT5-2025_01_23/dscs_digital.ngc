    <Tab Name="DSCS alt">

		<Widget Type="Box" Caption="COMAC1_SET" X="0" Y="0" Width="2" Align="Top Left">

			<Widget Type="Check" Caption="OUT_EN" X="0" Y="0">
				<TypesIf Type="Pset" Target="3,1,1"/>
			</Widget>
			<Widget Type="Box" Caption="OUT_CLK_A" X="0" Y="1" Width="2" Align="Top Left">
			<Widget Type="Bitfield" Caption="OUT_CLK_A" X="0" Y="1" Bits="0:1" Horizontal="2" ReadOnly="0">
				<TypesIf Type="Pset" Target="3,1,2"/>
			</Widget>
			</Widget>
			<Widget Type="Box" Caption="OUT_CLK_B" X="0" Y="2" Width="2" Align="Top Left">

			<Widget Type="Bitfield" Caption="OUT_CLK_B" X="0" Y="2" Bits="0:1" Horizontal="2" ReadOnly="0">
				<TypesIf Type="Pset" Target="3,1,3"/>
			</Widget>
			</Widget>
			<Widget Type="Box" Caption="OUT_D_A" X="0" Y="3" Width="2" Align="Top Left">

			<Widget Type="Bitfield" Caption="OUT_D_A" X="0" Y="3" Bits="0:1" Horizontal="2" ReadOnly="0">
				<TypesIf Type="Pset" Target="3,1,4"/>
			</Widget>
			</Widget>
			<Widget Type="Box" Caption="OUT_D_B" X="0" Y="4" Width="2" Align="Top Left">

			<Widget Type="Bitfield" Caption="OUT_D_B" X="0" Y="4" Bits="0:1" Horizontal="2" ReadOnly="0">
				<TypesIf Type="Pset" Target="3,1,5"/>
			</Widget>
			</Widget>
			<Widget Type="Check" Caption="IN_EN" X="0" Y="5">
				<TypesIf Type="Pset" Target="3,1,6"/>
			</Widget>
		</Widget>



		<Widget Type="Box" Caption="COMAC1_GET" X="2" Y="0" Width="2" Align="Top Left">
			<Widget Type="Check" Caption="CLK_IN" X="0" Y="10">
				<TypesIf Type="Pget" Target="3,1,2"/>
			</Widget>
			<Widget Type="Box" Caption="D_IN" X="2" Y="1" Width="2" Align="Top Left">
				<Widget Type="Bitfield" Caption="D_IN" X="0" Y="10" Bits="0:1:2" Horizontal="3" ReadOnly="0">
					<TypesIf Type="Pget" Target="3,1,3"/>
				</Widget>
			</Widget>
		</Widget>

